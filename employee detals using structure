#include<stdio.h>
struct employeee
{
     char name[200];
     int serial_no;
     int id;
      int age;
      int phone_no;
      int salary;
}
s[5];
int main(){
           int i;
    printf("enter the information of students\n");
    
    for(i=0;i<5;i++)
    {
        
        s[i].serial_no=i+1;
        printf("serial number: %d\n",s[i].serial_no);
        printf("enter the id :");
        scanf("%d",s[i].id);
        printf("enter the name :");
    
        scanf("%s",&s[i].name);
        printf("enter the age :");
        scanf("%d",&s[i].age);
        printf("enter the ph.no: ");
        scanf("%d",&s[i].phone_no);
        printf("enter the salary : ");
        scanf("%d",&s[i].salary);
    }
    
    printf("details of the student are");
    
    for(i=0;i<5;i++)
    {
        printf("serial no: %d\n ",s[i].serial_no);
        printf("employee id : %d\n",s[i].id);
        printf("name : %s\n",s[i].name);
        printf("age : %d\n",s[i].age);
        printf("phone number: %d\n",s[i].phone_no);
        printf("salary : %d\n",s[i].salary);
    }
    return 0;
}
